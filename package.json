{
  "name": "@a1yyss/y-comps",
  "version": "1.0.2",
  "main": "lib/index.js",
  "description": "test",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "keywords": [
    "ts",
    "tailwindcss",
    "components"
  ],
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "./src/**/*.less"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "release": "npm publish",
    "clean": "rm -rf es lib dist"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/A1yangshuang/@a1yyss/y-comps"
  },
  "bugs": {
    "url": "https://github.com/A1yangshuang/@a1yyss/y-comps/issues"
  },
  "homepage": "https://a1yangshuang.github.io/y-components/",
  "lint-staged": {
    "**/*.less": "stylelint",
    "*.{js,jsx,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-plugin-import": "^1.13.3",
    "classnames": "^2.3.1",
    "eslint": "^8.4.1",
    "less": "^3.13.1",
    "postcss-less": "^4.0.1",
    "react": "^16.12.0 || ^17.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "autoprefixer": "^9.8.8",
    "browserslist": "^4.19.1",
    "caniuse-lite": "^1.0.30001291",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "postcss": "^7.0.39",
    "prettier": "^2.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "yorkie": "^2.0.0"
  }
}
